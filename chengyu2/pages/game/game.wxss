  to {opacity: 0;}
}
.pk {
  text-align: center;
  position: relative;
  width: 750rpx;
  height: 45%;
}
.pk-top {
  background-color: #0C77D1;
  -webkit-animation: right .6s ease-in-out;
  animation: right .6s ease-in-out;
}
.pk-bottom {
  top: 200rpx;
  background-color: #C82B4D;
  -webkit-animation: left .6s ease-in-out;
  animation: left .6s ease-in-out;
}
.pk:after {
  content: '';
  position: absolute;
  left: 0;
  width: 0;
  height: 0;
}
.pk-top:after {
  bottom: 0;
  transform: translateY(100%);
  border-left: 750rpx solid #0C77D1;
  border-bottom: 200rpx solid transparent;
}
.pk-bottom:after {
  top: 0;
  transform: translateY(-100%);
  border-right: 750rpx solid #C82B4D;
  border-top: 200rpx solid transparent;
}
@-webkit-keyframes right
{
  from {
    right: 600px;
    top: 600px;
  }
  to {
    right: 0;
    top: 0;
  }
}
@keyframes right
{
  from {
    right: 600px;
    top: 600px;
  }
  to {
    right: 0;
    top: 0;
  }
}
@-webkit-keyframes left
{
  from {
    left: 600px;
    bottom: 600px;
  }
  to {
    left: 0;
    bottom: 0;
  }
}
@keyframes left
{
  from {
    left: 600px;
    bottom: 600px;
  }
  to {
    left: 0;
    bottom: 0;
  }
}
